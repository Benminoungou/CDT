<?xml version="1.0" encoding="UTF-8"?>
<dialog_box>
    <title>
        <text lang="en"> Regridding - NetCDF Data </text>
        <text lang="fr"> Regridding - Donnees NetCDF </text>
    </title>
    <widget>
        <combobox id="1">
            <option id="1">
                <text lang="en"> One NetCDF file </text>
                 <text lang="fr"> Un fichier NetCDF file </text>
            </option>
            <option id="2">
                <text lang="en"> Multiple NetCDF files </text>
            </option>
        </combobox>
        <combobox id="2">
            <option id="1">
                <text lang="en"> Aggregate </text>
            </option>
            <option id="2">
                <text lang="en"> Disaggregate </text>
            </option>
        </combobox>
        <label id="1">
            <text lang="en"> NetCDF Data </text>
        </label>
        <label id="2">
            <text lang="en"> NetCDF data file </text>
        </label>
        <label id="3">
            <text lang="en"> Directory containing the netcdf data (*.nc) </text>
        </label>
        <label id="4">
            <text lang="en"> NetCDF data sample file </text>
        </label>
        <label id="5">
            <text lang="en"> Use Grid from other NetCDF data </text>
        </label>
        <label id="6">
            <text lang="en"> NetCDF file to be used </text>
        </label>
        <label id="7">
            <text lang="en"> Aggregation method </text>
        </label>
        <label id="8">
            <text lang="en"> Action </text>
        </label>
        <label id="9">
            <text lang="en"> Method </text>
        </label>
        <label id="10">
            <text lang="en"> New grid boundaries and resolutions </text>
        </label>
        <label id="11">
            <text lang="en"> Longitude </text>
        </label>
        <label id="12">
            <text lang="en"> Latitude </text>
        </label>
        <label id="13">
            <text lang="en"> Directory to save results </text>
        </label>
    </widget>
    <help>
        <content id="1">
            <status>
                <text lang="en"> Minimum longitude in degree decimal </text>
            </status>
            <tooltip>
                <text lang="en"> Minimum longitude in degree decimal </text>
            </tooltip>
        </content>
        <content id="2">
            <status>
                <text lang="en"> Maximum longitude in degree decimal </text>
            </status>
            <tooltip>
                <text lang="en"> Maximum longitude in degree decimal </text>
            </tooltip>
        </content>
        <content id="3">
            <status>
                <text lang="en"> Resolution in degree decimal </text>
            </status>
            <tooltip>
                <text lang="en"> Resolution in degree decimal </text>
            </tooltip>
        </content>
        <content id="4">
            <status>
                <text lang="en"> Minimum latitude in degree decimal </text>
            </status>
            <tooltip>
                <text lang="en"> Minimum latitude in degree decimal </text>
            </tooltip>
        </content>
        <content id="5">
            <status>
                <text lang="en"> Maximum latitude in degree decimal </text>
            </status>
            <tooltip>
                <text lang="en"> Maximum latitude in degree decimal </text>
            </tooltip>
        </content>
        <content id="6">
            <status>
                <text lang="en"> Enter the full path to directory to save results </text>
            </status>
            <tooltip>
                <text lang="en"> Enter the full path to directory to save results </text>
            </tooltip>
        </content>
        <content id="7">
            <status>
                <text lang="en"> or browse here </text>
            </status>
            <tooltip>
                <text lang="en"> or browse here </text>
            </tooltip>
        </content>
    </help>
    <message>
        <content id="1">
            <text lang="en"> NetCDF regridding finished successfully </text>
        </content>
        <content id="2">
            <text lang="en"> NetCDF regridding failed </text>
        </content>
        <content id="3">
            <text lang="en"> Regrid netCDF data ... </text>
        </content>
        <content id="4">
            <text lang="en"> Directory to save results is missing </text>
        </content>
        <content id="5">
            <text lang="en"> Unable to read NetCDF file to be regridded </text>
        </content>
        <content id="6">
            <text lang="en"> Unable to read the sample NetCDF file </text>
        </content>
        <content id="7">
            <text lang="en"> Unable to read NetCDF file whose grid will be used </text>
        </content>
        <content id="8">
            <text lang="en"> Nothing to do the files have the same grid </text>
        </content>
        <content id="9">
            <text lang="en"> Nothing to do the new grid is finer than the grid of NetCDF data </text>
        </content>
        <content id="10">
            <text lang="en"> Nothing to do the new grid is coarser than the grid of NetCDF data </text>
        </content>
        <content id="11">
            <text lang="en"> No NetCDF files found </text>
        </content>
    </message>
</dialog_box>
