<?xml version="1.0" encoding="UTF-8"?>
<dialog_box>
    <title>
        <text lang="en"> NetCDF Data - Settings </text>
        <text lang="fr"> Données NetCDF - Paramètres </text>
    </title>
    <widget>
        <label id="1">
            <text lang="en"> NetCDF data sample file </text>
            <text lang="fr"> Fichier exemple de données NetCDF </text>
        </label>
        <label id="2">
            <text lang="en"> NetCDF data filenames format </text>
            <text lang="fr"> Format des fichiers de données NetCDF </text>
        </label>
        <label id="3">
            <text lang="en"> Replace the year, month or day by\n </text>
            <text lang="fr"> Remplacer l’année, le mois ou le jour par\n </text>
        </label>
        <label id="4">
            <text lang="en"> Year: %Y; Month: %M; Day: %D\n </text>
            <text lang="fr"> Année: %Y; Mois: %M; Jour: %D\n </text>
        </label>
        <label id="5">
            <text lang="en"> Example:\n </text>
            <text lang="fr"> Exemple:\n </text>
        </label>
    </widget>
    <help>
        <content id="1">
            <status>
                <text lang="en"> File containing a sample of the NetCDF data </text>
                <text lang="fr"> Fichier contenant un exemple des données NetCDF </text>
            </status>
            <tooltip>
                <text lang="en"> Browse file if not listed </text>
                <text lang="fr"> Parcourir le fichier si non répertorié </text>
            </tooltip>
        </content>
        <content id="2">
            <status>
                <text lang="en"> Enter the filename format of NetCDF data (see below) </text>
                <text lang="fr"> Entrez le format de nom de fichier des données NetCDF (voir ci-dessous) </text>
            </status>
            <tooltip>
                <text lang="en"> Enter the filename format of NetCDF data (see below) </text>
                <text lang="fr"> Entrez le format de nom de fichier des données NetCDF (voir ci-dessous) </text>
            </tooltip>
        </content>
    </help>
    <message>
        <content id="1">
            <text lang="en"> You have to provide a sample file </text>
            <text lang="fr"> Vous devez fournir un fichier exemple </text>
        </content>
    </message>
</dialog_box>